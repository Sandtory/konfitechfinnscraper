{
    "title": "Finn.no Job Scraper",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "searchUrl": {
            "title": "Search URL",
            "type": "string",
            "description": "The Finn.no job search URL to start scraping from",
            "editor": "textfield",
            "default": "https://www.finn.no/job/fulltime/search.html?occupation=0.23&occupation=0.22"
        },
        "maxJobs": {
            "title": "Maximum Jobs",
            "type": "integer",
            "description": "Maximum number of jobs to scrape",
            "editor": "number",
            "default": 10,
            "minimum": 1,
            "maximum": 1000
        }
    },
    "required": ["searchUrl"]
}
